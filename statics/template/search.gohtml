<html lang="fa">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <link rel="stylesheet" href="/statics/css/main.css">
    <link rel="stylesheet" href="/statics/css/tailwind.css">
    <title>
    </title>
    <meta name="description" content="مرجع داستان های سکسی تصویری - کمیک سکسی - عکس های لو رفته - سکسی داستانی کمیک ترجمه شده اختصاصی - داستان های سکسی ترجمه شده">
  </head>
  <body class="bg-[#f7f8fa] text-gray-400 antialiased font-regular max-w-2xl mx-auto" style="overflow-x:clip;float:right;min-width:100%;direction:rtl" data-new-gr-c-s-check-loaded="8.899.0" data-gr-ext-installed="" cz-shortcut-listen="true" data-gr-ext-disabled="forever">
    <div class="header w-full h-14 flex flex-row px-4 justify-center items-center">
      <h1 class="font-bold text-xl text-gray-900">سرراست
      </h1>
    </div>
    <div class="max-w-2xl mx-auto"> 
      <div class="max-w-2xl mx-auto">
        <div class="bg-white rounded-t-4xl  pa-3">
          <form action="/search" mehtod="GET" style="">
            <br>
            <label class="my-4 block" style="margin-top: ;">
              <div class="mb-1 text-sm font-semibold text-gray-800 pt-2 pb-1" style="font-size: 17;margin-right: 10;">
                بگرد دنبال چیزی که میخوای  ...
              </div>
              <input type="text" name="query" value="{{.Query}}" required="" placeholder="اینجا بنویس" class="focus:outline-none border focus:ring-2 ring-primary-200 w-full p-2 rounded-3xl text-base placeholder-gray-400 placeholder-opacity-100 focus:border-primary-200 focus:outline-none border-gray-300" style="margin-top: 15;">
            </label>
            <button type="submit" class="items-center text-gray-800 transition-all  font-semibold rounded-xl outline-none w-full sm:w-48 py-2 pl-5 pr-5 mb-3 sm:mb-0 h-10 active:scale-90 hover:bg-opacity-90 focus:ring-2 active:bg-opacity-80 bg-[#f7cc5f] ring-[#f7cc5f]" style="margin-right: 7;">
              پیدا کن
            </button>
          </form>
          <hr>
        </div>
        {{if .Query}}
            {{if .Comics}}
                <div class="flex flex-col mt-2.5 box-border min-h-32 px-4">
                <div class="mb-5">
                    <button class="outline-none transition-colors duration-150 ease-out select-none flex items-center box-border border-solid py-1.5 h-8 pr-2.5 pl-2 space-x-1 flex rounded-lg active:scale-90 bg-white border font-medium">
                    <div class="w-4 h-4 text-gray-800">
                        <svg width="100%" height="100%" viewBox="0 0 15 15" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M1.875 4.375H13.125" stroke="currentColor" stroke-width="1.5" stroke-linecap="round">
                        </path>
                        <path d="M3.75 7.5H11.25" stroke="currentColor" stroke-width="1.5" stroke-linecap="round">
                        </path>
                        <path d="M6.25 10.625H8.75" stroke="currentColor" stroke-width="1.5" stroke-linecap="round">
                        </path>
                        </svg>
                    </div>
                    <div class="text-gray-800 text-base font-medium">به ترتیب
                    </div>
                    <div class="text-white bg-gray-800 rounded-full py-0.5 px-2 font-medium text-[.625rem]">آپدیت
                    </div>
                    </button>
                </div>
                <div class="">
                    {{range $comic := .Comics}}
                        <div>
                            <a href="/comic/{{$comic.ID}}">
                                <div class="bg-white  p-1 flex flex-row-reverse mb-4 shadow hover:shadow-lg ring-gray-900">
                                <img src="{{$comic.CoverPath}}" loading="lazy" class="w-[180px] min-w-180px  h-[240px] hidden sm:block" alt="" width="180" height="240">
                                <div class="p-2 w-full px-2 sm:px-6">
                                    <div class="py-2 text-xs flex justify-between">
                                    <div>
                                    {{$comic.Status}}
                                    </div>
                                    <div class="text-[.625rem]">
                                    {{$comic.CreatedAt | HowManyAgo}}
                                    </div>
                                    </div>
                                    <h3 class="text-gray-900 mt-4">
                                    {{$comic.Name}}
                                    </h3>
                                    <p class="recommended-description text-gray-500 text-sm pt-2 max-w-[320px]">
                                    {{$comic.Description}}
                                    </p>
                                    <div class="h-12 bg-[#fef6e3] rounded-lg flex items-center px-3 mt-8">
                                    <h4 class="text-gray-400">قسمت {{$comic.NumberOfEpisodes}}
                                    </h4>
                                    </div>
                                </div>
                                </div>
                            </a>
                        </div>
                    {{end}}
                         
                    {{template "_paging" .PagingData}}
                </div>
                </div>
            {{else}}
                <div class="flex flex-col mt-2.5 box-border min-h-32 px-4">
                    <div class="mb-5">
                        <button class="outline-none transition-colors duration-150 ease-out select-none flex items-center box-border border-solid py-1.5 h-8 pr-2.5 pl-2 space-x-1 flex rounded-lg active:scale-90 bg-white border font-medium">
                        <div class="w-4 h-4 text-gray-800">
                            <svg width="100%" height="100%" viewBox="0 0 15 15" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M1.875 4.375H13.125" stroke="currentColor" stroke-width="1.5" stroke-linecap="round">
                            </path>
                            <path d="M3.75 7.5H11.25" stroke="currentColor" stroke-width="1.5" stroke-linecap="round">
                            </path>
                            <path d="M6.25 10.625H8.75" stroke="currentColor" stroke-width="1.5" stroke-linecap="round">
                            </path>
                            </svg>
                        </div>
                        <div class="text-gray-800 text-base font-medium">به ترتیب
                        </div>
                        <div class="text-white bg-gray-800 rounded-full py-0.5 px-2 font-medium text-[.625rem]">مرتبط ترین
                        </div>
                        </button>
                    </div>
                    <div class="">
                        <div>
                            <div class="bg-white  p-1 flex flex-row-reverse mb-4 shadow hover:shadow-lg ring-gray-900">
                            <img src="/api/img/series/she-is-working-out/thumb.webp" class="w-[130px] min-w-130px  h-[225px] rounded-l-2xl sm:hidden" alt="" width="130" height="225">
                            <div class="p-2 w-full px-2 sm:px-6">
                                <div class="py-2 text-xs flex justify-between">
                                </div>
                                <p class="" style="font-size: 20;">چیزی پیدا نشد ):
                                </p>
                            </div>
                            </div>
                        </div>
                    </div>
                </div>
            {{end}}
        {{end}}
      </div> 
    </div>
    <div class="mt-16">
    </div>
    <div>
      <div class="fixed  max-w-2xl left-0 right-0 flex flex-col bottom-0 z-60 mx-auto max-w-3xl h-12 bg-white bg-opacity-95 shadow-bottom-fixed-box rounded-t-4xl border border-b-0">
        <div class="h-full mx-auto w-full flex flex-row">
          <a class="w-full h-full" href="/">
            <button type="button" class="flex flex-col w-full h-full justify-end text-xs items-center active:scale-90 text-gray-900 relative">
              <svg width="1em" height="1em" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" class="h-5 w-5">
                <path d="M12 18v-3M10.07 2.82L3.14 8.37c-.78.62-1.28 1.93-1.11 2.91l1.33 7.96c.24 1.42 1.6 2.57 3.04 2.57h11.2c1.43 0 2.8-1.16 3.04-2.57l1.33-7.96c.16-.98-.34-2.29-1.11-2.91l-6.93-5.54c-1.07-.86-2.8-.86-3.86-.01z" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round">
                </path>
              </svg>
              <div class="border-t-2 border-primary rounded-t-md fill-current text-brand-primary w-10 mt-2.5 -mx-2 opacity-0">
              </div>
            </button>
          </a>
          <a class="w-full h-full" href="/schedule">
            <button class="flex flex-col w-full h-full justify-center text-xs items-center active:scale-90 text-gray-900 relative">
              <div class="h-5 w-5">
                <svg xmlns="http://www.w3.org/2000/svg" width="1em" height="1em" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" class="h-5 w-5">
                  <rect x="3" y="4" width="18" height="18" rx="2" ry="2">
                  </rect>
                  <line x1="16" y1="2" x2="16" y2="6">
                  </line>
                  <line x1="8" y1="2" x2="8" y2="6">
                  </line>
                  <line x1="3" y1="10" x2="21" y2="10">
                  </line>
                </svg>
                <div class="border-t-2 border-primary rounded-t-md fill-current text-brand-primary w-10 mt-2.5 -mx-2 opacity-0">
                </div>
              </div>
            </button>
          </a>
          <button type="button" class="flex flex-col w-full h-full justify-end text-xs items-center active:scale-90 text-gray-900 relative">
            <svg width="1em" height="1em" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" class="h-5 w-5">
              <path d="M11.5 21a9.5 9.5 0 100-19 9.5 9.5 0 000 19zM22 22l-2-2" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round">
              </path>
            </svg>
            <div class="border-t-2 border-primary rounded-t-md fill-current text-brand-primary w-10 mt-2.5 -mx-2 opacity-0">
            </div>
          </button>
          <a class="w-full h-full" href="/login">
            <button type="button" class="flex flex-col w-full h-full justify-end text-xs items-center active:scale-90 text-gray-900 relative">
              <img alt="undefined's avatar" srcset="undefined?width=256 480w, undefined?width=480 750w, undefined?width=750 1080w, undefined" class="h-5 w-5 rounded-full ring-1 ring-gray-900 hidden">
              <div class="h-5 w-5">
                <svg width="100%" height="100%" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M10.0999 10.65C10.0416 10.6417 9.9666 10.6417 9.89993 10.65C8.43327 10.6 7.2666 9.4 7.2666 7.925C7.2666 6.41667 8.48327 5.19167 9.99993 5.19167C11.5083 5.19167 12.7333 6.41667 12.7333 7.925C12.7249 9.4 11.5666 10.6 10.0999 10.65Z" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round">
                  </path>
                  <path d="M15.6166 16.15C14.1333 17.5083 12.1666 18.3333 9.99997 18.3333C7.8333 18.3333 5.86663 17.5083 4.3833 16.15C4.46663 15.3667 4.96663 14.6 5.8583 14C8.14163 12.4833 11.875 12.4833 14.1416 14C15.0333 14.6 15.5333 15.3667 15.6166 16.15Z" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round">
                  </path>
                  <path d="M10.0001 18.3333C14.6025 18.3333 18.3334 14.6024 18.3334 10C18.3334 5.39763 14.6025 1.66667 10.0001 1.66667C5.39771 1.66667 1.66675 5.39763 1.66675 10C1.66675 14.6024 5.39771 18.3333 10.0001 18.3333Z" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round">
                  </path>
                </svg>
              </div>
              <div class="border-t-2 border-primary rounded-t-md fill-current text-brand-primary w-10 mt-2.5 -mx-2 opacity-0">
              </div>
            </button>
          </a>
        </div>
      </div>
    </div>
  </body>
</html>
